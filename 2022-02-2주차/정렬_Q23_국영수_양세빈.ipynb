    "import sys\n",
    "n = int(sys.stdin.readline())\n",
    "li=[]\n",
    "for i in range(n):\n",
    "    li.append(list(sys.stdin.readline().split()))\n",
    "\n",
    "li=sorted(li, key=lambda x : (-int(x[1]), int(x[2]), -int(x[3]),x[0]))\n",
    "\n",
    "for i in li:\n",
    "    print(i[0])"
   ]
